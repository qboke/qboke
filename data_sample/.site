name        : Another QBoke
subhead     : Blogging in a easy way.
keywords    : [QBoke, Git, Markdown, Blog]
description : Blogging by your hands.
timezone    : Asia/Shanghai
lang   : zh_CN
rss    :
theme  : default
linage : 20
opts   :
    menu :
        - name : Home
          url  : /

        - name : About
          url  : /about.html

    sidebar :
        - name : ""
          type : text
          content : |
            <center>Welcom to my blog.</center>

        - name : Donate
          type : text
          content : |
            <center><a href="https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&hosted_button_id=FJEEDWPYTCNBC">
            	<img src="https://www.paypalobjects.com/en_GB/i/btn/btn_donate_LG.gif" />
            </a></center>

        - name : Tag List
          type : tag_list

        - name : Blogroll
          type : blogroll
          content :
              - name : QBoke
                desc : Blogging by your hands
                url  : http://qboke.org

              - name : Kernel
                desc : Useful things for Chinese Linuxers.
                url  : http://kernel.cn

    footer : |
        You are appreciated to <a href="https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&hosted_button_id=FJEEDWPYTCNBC">help us</a>!

    signature :
        enable   : true
        position : bottom # top | bottom
